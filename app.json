{
  "name": "TimeSheet",
  "description": "TimeSheet generates invoices on Zipbooks based on time entries at Toggl.",
  "keywords": ["timesheet", "time tracking", "zipbooks", "toggl"],
  "website": "https://github.com/mr-php/timesheet",
  "repository": "https://github.com/mr-php/timesheet",
  "env": {
    "APP_ADMIN_EMAIL": {
      "description": "Admin email address.",
      "value": "admin@mailinator.com"
    },
    "APP_COOKIE_VALIDATION_KEY": {
      "description": "Cookie validation key.",
      "generator": "secret"
    },
    "APP_NAME": {
      "description": "Application name.",
      "value": "timesheet"
    },
    "APP_TITLE": {
      "description": "Application title.",
      "value": "TimeSheet"
    },
    "APP_PASSWORD": {
      "description": "Access password.",
      "value": "secret"
    },
    "APP_PRETTY_URLS": {
      "description": "Pretty URLs.",
      "value": "1"
    },
    "APP_TIMEZONE": {
      "description": "Time zone.",
      "value": "Australia/Adelaide"
    },
    "YII_ENV": {
      "description": "Yii Environment.",
      "value": "prod"
    },
    "YII_DEBUG": {
      "description": "Yii Debug.",
      "value": "0"
    },
    "HEROKU": {
      "description": "Heroku (must be set).",
      "value": "1"
    }
  },
  "addons": [
    "heroku-postgresql"
  ]
}
